{
  "name": "friend-finder-server",
  "version": "1.0.0",
  "description": "Server for the Friend Finder application",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "seed": "node src/seed/seed-users.js",
    "client": "cd ../client && npm run start",
    "client-install": "cd ../client && npm install",
    "client-build": "cd ../client && npm run build",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm run client-install && npm run client-build",
    "dev-full": "concurrently \"npm run dev\" \"npm run client\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^5.10.9",
    "socket.io": "^3.1.2"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "nodemon": "^2.0.4"
  },
  "keywords": [
    "friend-finder",
    "social",
    "matching",
    "messaging"
  ],
  "author": "Your Name",
  "license": "ISC"
}
